@model IEnumerable<MVCDemo.Models.Course>

@{
    ViewBag.Title = "CList";
}

<h2>CList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Capacity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enrolled)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnEnrolled)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Capacity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enrolled)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnEnrolled)
        </td>
        <td>
            @Html.ActionLink("Enroll", "Enroll", new { id=item.CourseID })
        </td>
    </tr>
}

</table>
